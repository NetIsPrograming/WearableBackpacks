{
  "schemaVersion": 1,
  "id": "wearablebackpacks",
  "name": "Wearable Backpacks",
  "version": "$version",
  "description": "Adds beautiful, balanced backpacks",
  "license": "MIT",
  "depends": {
    "fabricloader": ">=0.15.7",
    "minecraft": "1.20.1",
    "java": ">=17"
  },
  "environment": "*",
  "entrypoints": {
    "client": [
      "dev.sapphic.wearablebackpacks.client.BackpacksClient",
      "dev.sapphic.wearablebackpacks.client.BackpackClientNetwork"
    ],
    "main": [
      "dev.sapphic.wearablebackpacks.BackpackMod",
      "dev.sapphic.wearablebackpacks.advancement.BackpackCriteria",
      "dev.sapphic.wearablebackpacks.event.BackpackEntityEvents",
      "dev.sapphic.wearablebackpacks.loot.BackpackLootFunctions",
      "dev.sapphic.wearablebackpacks.network.BackpackServerNetwork",
      "dev.sapphic.wearablebackpacks.stat.BackpackStats"
    ]
  },
  "mixins": [
    "mixins.server.json",
    {
      "config": "mixins.client.json",
      "environment": "client"
    }
  ],
  "authors": [
    "Chloe Dawn",
    "copygirl",
    "asie",
    "NetUserGet"
  ],
  "contact": {
    "homepage": "https://bit.ly/33owdaZ",
    "sources": "https://git.io/fjskt",
    "issues": "https://git.io/fjskq"
  }
}
